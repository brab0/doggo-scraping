{
	"goto" : {
		"url" : "http://editoraunicamp.com.br/",
		"iterate" : {
			"in" : "document.querySelectorAll('.itens_menu a')",
			"set" : {
				"property" : "categories",
				"set" : [{
					"property" : "link",
					"from" : "document.querySelector('.itens_menu a').getAttribute('href')"
				}, {
					"property" : "title",
					"from" : "document.querySelector('.itens_menu a').textContent"
				}, {
					"property" : "items",
					"goto" : {
						"url" : "document.querySelector('.itens_menu a').getAttribute('href')",
						"iterate" : {
							"in" : "document.querySelector('.caixa_produtos .box a')",
							"goto" : {
								"url" : "document.querySelector('.caixa_produtos .box a').getAttribute('href')",
								"set" : [{
									"property" : "image",
									"from" : "document.querySelector('.caixa_produtos_esquerda_foto .foto_detalhe a').getAttribute('href')"
								}, {
									"property" : "title",
									"from" : "document.querySelector('.caixa_produtos_direita h2').textContent"
								}, {
									"property" : "author",
									"from" : "document.querySelector('.caixa_produtos_direita p').textContent"
								}]
							}
						}
					}
				}]
			}
		}
	}
}
