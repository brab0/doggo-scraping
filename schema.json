{
	"baseUrl" : "http://editoraunicamp.com.br/",
	"template" : {
		"properties" : [{
			"id" : "categories",
			"iterate" : "document.querySelectorAll('.itens_menu a').getAttribute('href')",
			"properties" : [{
				"id" : "link",
				"keep" : "document.querySelectorAll('.itens_menu a').getAttribute('href')"
			}, {
				"id" : "title",
				"keep" : "document.querySelectorAll('.itens_menu a').textContent"
			}, {
				"id" : "items",
				"iterate" : "document.querySelector('.caixa_produtos .box a').getAttribute('href')",
				"goto" : [
					"document.querySelectorAll('.itens_menu a').getAttribute('href')",
					"document.querySelector('.caixa_produtos .box a').getAttribute('href')"
				],
				"properties" : [{
					"keep" : "document.querySelector('.caixa_produtos_esquerda_foto .foto_detalhe a').getAttribute('href')",
					"id" : "image"
				}, {
					"keep" : "document.querySelector('.caixa_produtos_direita h2').textContent",
					"id" : "title"
				}, {
					"keep" : "document.querySelector('.caixa_produtos_direita p:nth-child(0)').textContent",
					"id" : "author"
				}]
			}]
		}]
	}
}
